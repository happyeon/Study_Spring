<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean으로 2명 등록 -->	
	<bean id="p1" class="com.test02.Address">
		<constructor-arg index="0" value="김혜수" />
		<constructor-arg index="1" value="서울시 마포구" />
		<constructor-arg index="2" value="010-3344-2433" />
	</bean>
	
	<bean id="p2" class="com.test02.Address">
		<constructor-arg name="name" value="이보영" />
		<constructor-arg name="addr" value="서울시 강남구" />
		<constructor-arg name="phone" value="010-3542-2788" />
	</bean>
	
</beans>
